{
  "name": "@bryter-client/applications",
  "version": "0.0.1",
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "generate:graphql": "graphql-codegen",
    "start:client:dev": "vite --port 8080",
    "start:client:preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@graphql-codegen/typescript": "^2.4.10",
    "@graphql-codegen/typescript-operations": "^2.3.7",
    "@graphql-codegen/typescript-vue-urql": "^2.2.11",
    "@urql/devtools": "^2.0.3",
    "@urql/vue": "^0.6.2",
    "graphql": "15.7.2",
    "pinia": "^2.0.13",
    "timeago.js": "^4.0.2",
    "vue": "^3.2.33",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.27",
    "@vicons/material": "^0.12.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.20",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.7",
    "cypress": "^9.5.4",
    "eslint": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^8.2.0",
    "jsdom": "^19.0.0",
    "naive-ui": "^2.28.2",
    "postcss": "^8.4.13",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.24",
    "typescript": "~4.6.3",
    "vfonts": "^0.0.3",
    "vite": "^2.9.5",
    "vitest": "^0.9.3",
    "vue-tsc": "^0.34.7"
  }
}
